--------------------------------------------------------------#//#// Define any local options for the make tools #//     (ie. LCFLAGS, LLFLAGS, LLIBS, LINCS)#//#//------------------------------------------------------------------------LLIBS=$(LLIBS) LINCS=$(LINCS) -I_gen -I$(DEPTH)/include -I$(PUBLIC)\dbm -I$(PUBLIC)\imgLCFLAGS	= $(LCFLAGS) /Fr$*.sbrGARBAGE	= $(GARBAGE) *.sbr!ifdef ALL_UNICODE_TABLE!if "$(MOZ_BITS)" == "32"RCFLAGS=$(RCFLAGS) /DALL_UNICODE_TABLE /DXP_WIN32!elseRCFLAGS=$(RCFLAGS) /DALL_UNICODE_TABLE!endif!else!if "$(MOZ_BITS)" == "32"RCFLAGS=$(RCFLAGS) /DXP_WIN32!endif!endif#//------------------------------------------------------------------------#//#// Include the common makefile rules#//#//------------------------------------------------------------------------!ifdef MOZ_JAVAinclude <$(DEPTH)/sun-java/config/rules.mak>!elseinclude <$(DEPTH)/config/rules.mak>!endifinstall:: $(DLL) $(LIBRARY)    $(MAKE_INSTALL) .\$(OBJDIR)\$(DLLNAME).dll $(DIST)\bin    $(MAKE_INSTALL) .\$(OBJDIR)\$(DLLNAME).lib $(DIST)\lib