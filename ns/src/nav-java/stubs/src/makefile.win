/------------------------------------------------------------------------!if "$(MOZ_BITS)" == "16"OBJS=\.\$(OBJDIR)\STUBNJ.OBJ \$(NULL)!elseOBJS =                        \    .\$(OBJDIR)\stubnj.obj           \    $(NULL)!endif#//------------------------------------------------------------------------#//#// Define any Public Targets here (ie. PROGRAM, LIBRARY, DLL, ...)#// (these must be defined before the common makefiles are included)#//#//------------------------------------------------------------------------LIBRARY= .\$(OBJDIR)\$(LIBNAME).lib#//------------------------------------------------------------------------#//#// Define any local options for the make tools #//     (ie. LCFLAGS, LLFLAGS, LLIBS, LINCS)#//#//------------------------------------------------------------------------#//------------------------------------------------------------------------#//#// Include the common makefile rules#//#//------------------------------------------------------------------------include <$(DEPTH)/config/rules.mak>export::install:: $(LIBRARY)    $(MAKE_INSTALL) $(LIBRARY) $(DIST)\libclobber::