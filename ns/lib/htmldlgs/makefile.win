---------------------#//#// Define any Public Targets here (ie. PROGRAM, LIBRARY, DLL, ...)#// (these must be defined before the common makefiles are included)#//#//------------------------------------------------------------------------LIBRARY=.\$(OBJDIR)\$(LIBNAME).lib#//------------------------------------------------------------------------#//#// install headers#//#//------------------------------------------------------------------------INSTALL_DIR=$(PUBLIC)\htmldlgsINSTALL_FILE_LIST=htmldlgs.h#//------------------------------------------------------------------------#//#// Include the common makefile rules#//#//------------------------------------------------------------------------include <$(DEPTH)/config/rules.mak>install:: $(LIBRARY)    $(MAKE_INSTALL) $(LIBRARY) $(DIST)\libexport:: INSTALL_FILES