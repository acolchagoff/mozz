----------------------------------------------------------#//#// Define any Public Targets here (ie. PROGRAM, LIBRARY, DLL, ...)#// (these must be defined before the common makefiles are included)#//#//------------------------------------------------------------------------DLL=.\$(OBJDIR)\$(DLLNAME).dllLINCS=$(LINCS) -I_gen -I$(PUBLIC)\dbm -I$(DEPTH)\include -I$(XPDIST)\applet#// End of MOZ_BITS == 32!endif #//------------------------------------------------------------------------#//#// Specify any "command" targets. (ie. DIRS, INSTALL_FILES, ...)#// (these must come before the common makefiles are included)#// #// DIRS          - There are subdirectories to process#//#//------------------------------------------------------------------------DIRS=unicode#//------------------------------------------------------------------------#//#// Include the common makefile rules#//#//------------------------------------------------------------------------include <$(DEPTH)/config/rules.mak>#// More Win32 only stuff...!if "$(MOZ_BITS)" == "32"install:: $(DLL) $(LIBRARY)    $(MAKE_INSTALL) .\$(OBJDIR)\$(DLLNAME).dll $(DIST)\bin    $(MAKE_INSTALL) .\$(OBJDIR)\$(DLLNAME).lib $(DIST)\lib!endif