obj   \    .\$(OBJDIR)\h_page.obj   \    .\$(OBJDIR)\hash.obj   \    .\$(OBJDIR)\hash_buf.obj   \    .\$(OBJDIR)\hsearch.obj   \    .\$(OBJDIR)\memmove.obj   \    .\$(OBJDIR)\mktemp.obj   \    .\$(OBJDIR)\ndbm.obj   \    .\$(OBJDIR)\snprintf.obj   \    .\$(OBJDIR)\strerror.obj   \    .\$(OBJDIR)\nsres.obj   \    $(NULL)#//------------------------------------------------------------------------#//#// Define any Public Targets here (ie. PROGRAM, LIBRARY, DLL, ...)#// (these must be defined before the common makefiles are included)#//#//------------------------------------------------------------------------LIBRARY	= .\$(OBJDIR)\$(LIBNAME).libLINCS = -I..\include#//------------------------------------------------------------------------#//#// Include the common makefile rules#//#//------------------------------------------------------------------------include <$(DEPTH)/config/rules.mak>CFLAGS = $(CFLAGS) -DMOZILLA_CLIENTinstall:: $(LIBRARY)    $(MAKE_INSTALL) $(LIBRARY) $(DIST)\lib